{
  "jupyter-lite-schema-version": 0,
  "jupyter-config-data": {
    "disabledExtensions": [
      "@jupyterlab/drawio-extension",
      "jupyterlab-kernel-spy",
      "jupyterlab-tour"
    ],
    "litePluginSettings": {
      "@jupyterlite/pyodide-kernel-extension:kernel": {
        "pipliteUrls": ["./pypi/all.json"],
        "loadPyodideOptions": {
          "packages": [
            "micropip", 
            "packaging", 
            "sqlite3",
            "typing-extensions"
          ]
        },
        "pyodideKernelOptions": {
          "disablePyodideDocstring": false
        }
      }
    }
  },
  "PipliteAddon": {
    "piplite_urls": [
      "content/packages/"
    ]
  }
}
